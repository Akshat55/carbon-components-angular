{
  "name": "carbon-components-angular",
  "version": "0.0.0",
  "description": "Next generation components",
  "main": "index.js",
  "scripts": {
    "build": "bash scripts/build.sh",
    "storybook": "start-storybook -s .storybook/public -p 6006",
    "docs:build": "typedoc",
    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint --fix 'src/**/*.ts'",
    "test": "ng test --no-watch",
    "test:watch": "ng test --watch",
    "build-storybook": "build-storybook -c .storybook -s .storybook/public -o dist/docs/storybook",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "ng": "ng",
    "ng:build": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build"
  },
  "husky": {
    "hooks": {
      "pre-push": "bash scripts/prepush.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "pkgRoot": "dist",
    "branches": [
      {
        "name": "master",
        "channel": "latest"
      },
      {
        "name": "next",
        "channel": "next"
      },
      {
        "name": "v3",
        "channel": "carbon-v3",
        "range": "3.x"
      },
      {
        "name": "v9",
        "channel": "carbon-v9",
        "range": "2.X"
      },
      {
        "name": "v10",
        "channel": "carbon-v10",
        "range": "4.x"
      }
    ],
    "tagFormat": "v${version}"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-components"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:IBM/carbon-components-angular.git"
  },
  "license": "Apache-2.0",
  "author": "IBM",
  "peerDependencies": {
    "@carbon/styles": "^1.4.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular-devkit/build-ng-packagr": "~0.901.15",
    "@angular-devkit/core": "9.1.15",
    "@angular/cli": "9.1.15",
    "@angular/common": "9.1.13",
    "@angular/compiler": "9.1.13",
    "@angular/compiler-cli": "9.1.13",
    "@angular/core": "9.1.13",
    "@angular/forms": "9.1.13",
    "@angular/platform-browser": "9.1.13",
    "@angular/platform-browser-dynamic": "9.1.13",
    "@angular/router": "9.1.13",
    "@babel/core": "7.4.3",
    "@carbon/styles": "1.4.0",
    "@carbon/themes": "11.0.1",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addon-notes": "5.3.19",
    "@storybook/addon-options": "5.3.19",
    "@storybook/addon-storysource": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/angular": "5.3.19",
    "@storybook/theming": "5.3.19",
    "@types/jasmine": "2.8.2",
    "@types/node": "^12.11.1",
    "babel-loader": "8.0.5",
    "chai": "4.2.0",
    "codelyzer": "^5.1.2",
    "commitizen": "4.2.4",
    "core-js": "2.6.11",
    "cz-conventional-changelog-components": "1.0.0",
    "element-closest-polyfill": "1.0.0",
    "exported-tests": "1.0.1",
    "file-type": "12.4.0",
    "gulp": "4.0.0",
    "gulp-concat": "2.6.1",
    "gulp-tap": "1.0.1",
    "husky": "1.3.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "6.3.16",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "lodash-es": "4.17.21",
    "ng-packagr": "^9.0.0",
    "rxjs": "6.6.7",
    "sass": "1.49.9",
    "sass-loader": "10.1.1",
    "semantic-release": "17.2.3",
    "ts-helpers": "1.1.2",
    "tslint": "5.15.0",
    "typedoc": "0.14.2",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "3.8.3",
    "whatwg-fetch": "3.0.0",
    "zone.js": "~0.10.2"
  },
  "dependencies": {
    "@carbon/icon-helpers": "10.30.0",
    "@carbon/icons": "11.4.0",
    "@carbon/telemetry": "0.1.0",
    "@carbon/utils-position": "1.1.4",
    "flatpickr": "4.6.13",
    "tslib": "^1.10.0"
  }
}
